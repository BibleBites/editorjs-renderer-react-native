{
    "expo": {
        "name": "EditorJS Render React Native",
        "slug": "editorjs-Render-react-native",
        "scheme": "editorjs-render",
        "version": "0.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "entryPoint": "./index.ts",
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.yourname.yourapp"
        },
        "web": {
            "bundler": "metro",
            "output": "static"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
