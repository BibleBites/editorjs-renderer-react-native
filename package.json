{
    "name": "@biblebites/editorjs-renderer-react-native",
    "version": "0.0.0",
    "export-properties": {
        "main": "./dist/index.js",
        "module": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
    },
    "scripts": {
        "prepare": "npm run build",
        "prepare-export-properties": "node prepare.js",
        "build": "tsup src/index.tsx --format esm,cjs --dts --minify --sourcemap --external react,react-native",
        "dev": "expo start --clear",
        "format": "biome check --write .",
        "lint": "biome lint . && tsc --noEmit",
        "test": "echo \"Error: no test specified\""
    },
    "author": "BibleBites",
    "license": "MIT",
    "description": "A React Native viewer for JSON created by EditorJs",
    "dependencies": {
        "html-entities": "^2.5.2",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.2",
        "tsup": "^8.4.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/react": "~19.0.10",
        "@types/react-native": "^0.72.8",
        "expo": "^53.0.20",
        "expo-router": "~5.0.7",
        "react-native-safe-area-context": "5.4.0",
        "react-native-web": "^0.20.0",
        "typescript": "~5.8.3"
    },
    "files": ["dist/*"]
}
