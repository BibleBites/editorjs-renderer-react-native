{
    "name": "@biblebytes/editorjs-renderer-react-native",
    "version": "0.0.0",
    "export-properties": {
        "main": "./index.ts",
        "module": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
    },
    "scripts": {
        "prepare": "node prepare.js && npm run build",
        "build": "tsup src/index.tsx --format esm,cjs --dts --minify --external react,react-native",
        "dev": "expo start --clear",
        "format": "biome check --write .",
        "lint": "biome lint . && tsc --noEmit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "BibleBytes",
    "license": "MIT",
    "description": "A React Native viewer for JSON created by EditorJs",
    "dependencies": {
        "html-entities": "^2.5.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.7",
        "tsup": "^8.4.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/react": "~18.3.12",
        "@types/react-native": "^0.72.8",
        "expo": "^52.0.33",
        "expo-router": "~4.0.16",
        "react-native-safe-area-context": "4.12.0",
        "react-native-web": "^0.19.13",
        "typescript": "^5.7.3"
    },
    "files": ["dist/*"]
}
