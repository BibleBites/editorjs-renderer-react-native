{
    "name": "@biblebites/editorjs-renderer-react-native",
    "version": "0.0.0",
    "export-properties": {
        "main": "./dist/index.js",
        "module": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
    },
    "scripts": {
        "prepare": "npm run build",
        "prepare-export-properties": "node prepare.js",
        "build": "tsup src/index.tsx --format esm,cjs --dts --minify --sourcemap --external react,react-native",
        "dev": "expo start --clear",
        "format": "biome check --write .",
        "lint": "biome lint . && tsc --noEmit",
        "test": "echo \"Error: no test specified\""
    },
    "author": "BibleBites",
    "license": "MIT",
    "description": "A React Native viewer for JSON created by EditorJs",
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-native": ">=0.72.0"
    },
    "dependencies": {
        "html-entities": "^2.5.2"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "@types/react": "~19.1.10",
        "@types/react-native": "^0.72.8",
        "expo": "^54.0.13",
        "expo-router": "~6.0.11",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-native": "0.81.4",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-web": "^0.21.0",
        "tsup": "^8.4.0",
        "typescript": "~5.9.2"
    },
    "files": [
        "dist/*"
    ]
}
